{
  "name": "vue-iconify",
  "version": "1.0.8",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daixu-cn/vue-iconify.git"
  },
  "bugs": "https://github.com/daixu-cn/vue-iconify/issues",
  "homepage": "https://github.com/daixu-cn/vue-iconify",
  "author": "daixu <daixu.cn@outlook.com> (https://daixu.cn/)",
  "description": "This is a component for introducing SVG icons in your Vue.js project, which allows you to easily integrate SVG icons into your application and add custom styles and animations to them.",
  "keywords": [
    "daixu",
    "svg",
    "icon",
    "vue-svg",
    "vue-icon"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js",
      "types": "./dist/src/index.d.ts"
    },
    "./global": "./dist/src/global.d.ts"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier": "prettier --write .",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.2.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "terser": "^5.30.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^2.0.12"
  }
}
